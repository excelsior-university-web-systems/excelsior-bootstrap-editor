{
    "apiVersion": 2,
    "name": "excelsior-bootstrap-editor/tabs",
    "title": "Tabs",
    "description": "Create tabs to organize information into separate sections.",
    "category": "excelsior-bootstrap-editor-blocks",
    "parent": ["excelsior-bootstrap-editor/container"],
    "example": {
        "attributes": {
            "cover": "images/tabs.webp"
        }
    },
    "attributes": {
        "cover": {
            "type": "string",
            "default": ""
        },
        "tabs": {
            "type": "array",
            "default": []
        },
        "tabStyle": {
            "type": "string",
            "default": "excelsior-tabs"
        },
        "activeTab": {
            "type": "string",
            "default": ""
        }
    },
    "providesContext": {
        "excelsior-bootstrap-editor/activeTab": "activeTab"
    },
    "supports": {
        "html": false
    }
}
