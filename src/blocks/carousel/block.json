{
    "apiVersion": 2,
    "name": "excelsior-bootstrap-editor/carousel",
    "title": "Carousel",
    "description": "Create Carousel for cycling through imagesâ€”like a slideshow.",
    "category": "excelsior-bootstrap-editor-blocks",
    "parent": ["excelsior-bootstrap-editor/container"],
    "example": {
        "attributes": {
            "cover": "admin/images/carousel.webp"
        }
    },
    "providesContext": {
        "excelsior-bootstrap-editor/inCarousel": "inCarousel",
        "excelsior-bootstrap-editor/activeSlide": "activeSlide"
    },
    "attributes": {
        "cover": {
            "type": "string",
