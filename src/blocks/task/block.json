{
    "apiVersion": 2,
    "name": "excelsior-bootstrap-editor/task",
    "title": "Task",
    "category": "excelsior-bootstrap-editor-blocks",
    "parent": ["excelsior-bootstrap-editor/task-group"],
    "supports": {
        "html": false
    },
    "attributes": {
        "task": {
            "type": "string"
        },
        "dueDayOfTheWeek": {
            "type": "string",
            "default": "Sunday"
        },
        "dueTime": {
            "type": "string",
            "default": "23:59"
        },
        "timezone": {
            "type": "string",
            "default": "ET"
        },
        "noDueDate": {
            "type": "boolean",
            "default": false
        }
    }
}
