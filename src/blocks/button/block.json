{
    "apiVersion": 2,
    "name": "excelsior-bootstrap-editor/button",
    "title": "Button",
    "description": "Create a button item.",
    "category": "excelsior-bootstrap-editor-blocks",
    "parent": ["excelsior-bootstrap-editor/buttons"],
    "supports": {
        "html": false
    },
    "attributes": {
        "href": {
            "type": "string",
            "default": "#"
        },
        "target": {
            "type": "string",
            "default": ""
        },
        "text": {
            "type": "string",
            "default": ""
        },
        "styleType": {
            "type": "string",
            "default": "btn-internal"
        },
        "subsequent": {
            "type": "boolean",
            "default": false
        }
    }
}
