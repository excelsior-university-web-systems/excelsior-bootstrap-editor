{
    "apiVersion": 2,
    "name": "excelsior-bootstrap-editor/accordion",
    "title": "Accordion",
    "description": "Create a collapsible component used to organize content into expandable sections.",
    "category": "excelsior-bootstrap-editor-blocks",
    "parent": ["excelsior-bootstrap-editor/container"],
    "supports": {
        "html": false
    },
    "example": {
        "attributes": {
            "cover": "public/images/accordion.webp"
        }
    },
    "attributes": {
        "cover": {
            "type": "string",
            "default": ""
        },
        "accordionHeadingLevel": {
            "type": "string",
            "default": "h2"
        }
    },
    "providesContext": {
        "excelsior-bootstrap-editor/accordionHeadingLevel": "accordionHeadingLevel"
    }
}