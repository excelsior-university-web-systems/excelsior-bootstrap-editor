{
    "apiVersion": 2,
    "name": "excelsior-bootstrap-editor/cards",
    "title": "Cards",
    "description": "Create cards to display content in a compact, organized format, often used for showcasing.",
    "category": "excelsior-bootstrap-editor-blocks",
    "parent": ["excelsior-bootstrap-editor/container"],
    "supports": {
        "html": false
    },
    "example": {
        "attributes": {
            "cover": "admin/images/cards.webp"
        }
    },
    "providesContext": {
        "excelsior-bootstrap-editor/cardBgColor": "bgColor"
    },
    "attributes": {
        "cover": {
            "type": "string",
            "default": ""
        },
        "colSize": {
            "type": "string",
            "default": "3"
        },
        "bgColor": {
            "type": "string",
            "default": ""
        }
    }
}
